{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "captain/card",
	"version": "0.1.0",
	"title": "Card",
	"category": "captain",
	"description": "Add a card with an image and caption.",
	"supports": {
		"html": false
	},
	"textdomain": "card",
	"editorScript": "file:./index.js",
	"attributes": {
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src",
			"default": ""
		}
	}
}
