{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "captain/grid-item",
	"version": "0.1.0",
	"title": "Grid item",
	"category": "widgets",
	"parent": ["captain/grid"],
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"textdomain": "grid-item",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"message": {
			"type": "string",
			"source": "text",
			"selector": "div",
			"default": ""
		},
		"align": {
			"type": "string",
			"source": "text",
			"default": ""
		}
	}
}
