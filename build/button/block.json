{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "captain/button",
  "version": "0.1.0",
  "title": "button",
  "category": "widgets",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "textdomain": "button",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "url": {
      "type": "string",
      "source": "attribute",
      "selector": "a",
      "attribute": "href"
    },
    "title": {
      "type": "string",
      "source": "attribute",
      "selector": "a",
      "attribute": "title"
    },
    "text": {
      "type": "string",
      "source": "html",
      "selector": "a"
    },
    "linkTarget": {
      "type": "string",
      "source": "attribute",
      "selector": "a",
      "attribute": "target"
    },
    "rel": {
      "type": "string",
      "source": "attribute",
      "selector": "a",
      "attribute": "rel"
    },
    "placeholder": {
      "type": "string"
    }
  }
}